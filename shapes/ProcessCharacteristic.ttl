@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh:    <http://www.w3.org/ns/shacl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex:    <http://www.example.org/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix pmd:   <https://w3id.org/pmd/co/> .
@prefix obo:   <http://purl.obolibrary.org/obo/> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix unit: <http://qudt.org/vocab/unit/> .

pmd:ProcessCharacteristicShape
    rdf:type sh:NodeShape ; 
    sh:targetClass pmd:PMD_0000008 ; # process characteristic
    sh:description "Shape which states that process characteristic refers to some quality influencing the output of a process. " ;
    sh:property
    [
        sh:path pmd:PMD_0020113 ; # refers to
        sh:class obo:BFO_0000019 ; # quality
    ] ;

    sh:property 
    [
        sh:path [ sh:inversePath pmd:PMD_0000009 ] ; # inverse of has process characteristic
        sh:class obo:BFO_0000015 ; # process
    ] ;

    sh:closed true ;
    sh:ignoredProperties ( rdf:type owl:topDataProperty owl:topObjectProperty rdfs:label) .