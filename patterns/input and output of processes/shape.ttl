@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pmd: <https://w3id.org/pmd/co/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix shape: <https://w3id.org/pmd/co/shapes/input_and_output_of_processes/> .
@prefix : shape: .
@base shape: .

shape:
  rdf:type owl:Ontology ;
  owl:imports sh: .

shape:manufacturing_process
    a sh:NodeShape ;
    sh:targetClass  pmd:PMD_0000833 ;   # Manufacturing process
    sh:property [            
        sh:path <http://purl.obolibrary.org/obo/OBI_0000293> ;      # has specified input
        sh:minCount 1 ;
    ] ;
    sh:property [            
        sh:path <http://purl.obolibrary.org/obo/OBI_0000299> ;       # has specified output
        sh:minCount 1 ;
    ] ;
   
   # sh:closed true ;
   # sh:ignoredProperties ( rdf:type owl:topDataProperty owl:topObjectProperty ) ;
    .
